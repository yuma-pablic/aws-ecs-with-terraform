{
  "containerDefinitions": [
    {
      "cpu": 0,
      "essential": true,
      "image": "{{ tfstate `module.network.data.aws_caller_identity.self.account_id` }}.dkr.ecr.ap-northeast-1.amazonaws.com/sbcntr-backend:v1",
      "logConfiguration": {
        "logDriver": "awsfirelens"
      },
      "name": "app",
      "portMappings": [
        {
          "appProtocol": "http",
          "containerPort": 80,
          "hostPort": 80,
          "name": "app-80-tcp",
          "protocol": "tcp"
        }
      ]
    },
    {
      "cpu": 0,
      "environment": [
        {
          "name": "LOG_BUCKET_NAME",
          "value": "dev-sbcntr-firelens"
        },
        {
          "name": "APP_ID",
          "value": "backend-def"
        },
        {
          "name": "AWS_ACCOUNT_ID",
          "value": "{{ tfstate `module.network.data.aws_caller_identity.self.account_id` }}"
        },
        {
          "name": "AWS_REGION",
          "value": "ap-northeast-1"
        },
        {
          "name": "LOG_GROUP_NAME",
          "value": "/aws/ecs/sbcntr-backend-def"
        }
      ],
      "essential": true,
      "firelensConfiguration": {
        "type": "fluentbit",
        "options": {
          "config-file-type": "file",
          "config-file-value": "/fluent-bit/custom.conf"
        }
      },
      "image": "{{ tfstate `module.network.data.aws_caller_identity.self.account_id` }}.dkr.ecr.ap-northeast-1.amazonaws.com/sbcntr-log:latest",
      "logConfiguration": {
        "logDriver": "awsfirelens",
        "options": {
          "Name": "s3",
          "bucket": "dev-sbcntr-firelens",
          "region": "ap-northeast-1",
          "total_file_size": "1M",
          "upload_timeout": "1m",
          "use_put_object": "On"
        }
      },
      "memoryReservation": 50,
      "name": "log_router",
      "user": "0"
    }
  ],
  "cpu": "1024",
  "executionRoleArn": "arn:aws:iam::{{ tfstate `module.network.data.aws_caller_identity.self.account_id` }}:role/dev-sbcntr-api-ecs-task-execution",
  "family": "sbcntr-backend-def",
  "ipcMode": "",
  "memory": "3072",
  "networkMode": "awsvpc",
  "pidMode": "",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}